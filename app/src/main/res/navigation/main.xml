<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/exampleFragment">
    <fragment
        android:id="@+id/exampleFragment"
        android:name="eu.darken.androidstarter.main.ui.main.MainFragment"
        tools:layout="@layout/main_fragment"
        android:label="@string/label_example_fragment">
        <argument
            android:name="exampleArgument"
            app:argType="string" />
        <action
            android:id="@+id/action_exampleFragment_to_settingsActivity"
            app:destination="@id/settingsActivity" />
    </fragment>
    <activity
        android:id="@+id/settingsActivity"
        android:name="eu.darken.androidstarter.settings.ui.SettingsActivity"
        android:label="SettingsActivity" />

</navigation>